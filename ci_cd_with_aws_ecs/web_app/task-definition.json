{
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "inferenceAccelerators": [],
    "containerDefinitions": [
        {
            "name": "site",
            "image": "",
            "cpu": 0,
            "essential": true,
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                  "awslogs-group": "github_actions_ecr_log_group",
                  "awslogs-region": "us-east-1",
                  "awslogs-stream-prefix": "site"
                    }
                },
            "portMappings": [
                {
                    "hostPort": 80,
                    "containerPort": 80,
                    "protocol": "tcp"
                }
                
            ],
            "privileged": false,
            "readonlyRootFilesystem": false
        }
    ],
    "volumes": [],
    "networkMode": "awsvpc",
    "memory": "512",
    "cpu": "256",
    "executionRoleArn": "arn:aws:iam::{{ACCOUNT_ID}}:role/ecsTaskExecutionRole",
    "family": "github_actions_ecr",
    "taskRoleArn": "",
    "placementConstraints": []
}
